<div class="row">
  <div class="col s3"></div>
  <div class="col s6">
    <h2>Create Username</h2>
  <%= form_for @user do |f| %>
    <%= @user.errors.full_messages.first if @user.errors.any? %><br />
    <%= f.label :title, "Username" %><br />
    <%= f.text_field :username %>
    <%= f.submit 'Create', class: 'btn' %>

  <% end %>
  </div>
  <div class="col s3"></div>
</div>
